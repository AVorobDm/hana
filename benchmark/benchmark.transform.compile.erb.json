<% hana = (1..50).step(2).to_a + (50..400).step(25).to_a %>
<% fusion = (1..50).step(2).to_a %>
<% mpl = (1..20).step(2).to_a %>

{
  "title": {
    "text": "Compile-time behavior of transform"
  },
  "series": [
    {
      "name": "Heterogeneous hana::tuple",
      "data": <%= time_compilation('transform/compile.hana.tuple.heterogeneous.erb.cpp', hana) %>
    }, {
      "name": "Homogeneous hana::tuple",
      "data": <%= time_compilation('transform/compile.hana.tuple.homogeneous.erb.cpp', hana) %>
    }, {
      "name": "hana::tuple_t (hana::Metafunction)",
      "data": <%= time_compilation('transform/compile.hana.tuple_t.metafunction.erb.cpp', hana) %>
    }, {
      "name": "mpl::vector",
      "data": <%= time_compilation('transform/compile.mpl.vector.erb.cpp', mpl) %>
    }, {
      "name": "Homogeneous fusion::vector",
      "data": <%= time_compilation('transform/compile.fusion.vector.homogeneous.erb.cpp', fusion) %>
    }, {
      "name": "Heterogeneous fusion::vector",
      "data": <%= time_compilation('transform/compile.fusion.vector.heterogeneous.erb.cpp', fusion) %>
    }
  ]
}