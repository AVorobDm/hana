<% hana = (1..50).step(2).to_a + (50..400).step(25).to_a %>
<% fusion = (1..50).step(2).to_a %>
<% mpl = (1..20).step(2).to_a %>

{
  "title": {
    "text": "Compile-time behavior of fold.right"
  },
  "series": [
    {
      "name": "Heterogeneous hana::tuple",
      "data": <%= time_compilation('fold_right/compile.hana.tuple.heterogeneous.erb.cpp', hana) %>
    }, {
      "name": "Homogeneous hana::tuple",
      "data": <%= time_compilation('fold_right/compile.hana.tuple.homogeneous.erb.cpp', hana) %>
    }, {
      "name": "hana::tuple_t with a hana::Metafunction",
      "data": <%= time_compilation('fold_right/compile.hana.tuple_t.metafunction.erb.cpp', hana) %>
    }

    <% if has_boost %>
    , {
      "name": "Heterogeneous fusion::vector",
      "data": <%= time_compilation('fold_right/compile.fusion.vector.heterogeneous.erb.cpp', fusion) %>
    }, {
      "name": "Homogeneous fusion::vector",
      "data": <%= time_compilation('fold_right/compile.fusion.vector.homogeneous.erb.cpp', fusion) %>
    }, {
      "name": "mpl::vector with a MPL metafunction class",
      "data": <%= time_compilation('fold_right/compile.mpl.vector.erb.cpp', mpl) %>
    }
    <% end %>
  ]
}